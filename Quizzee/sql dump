CREATE TABLE Users (
	user_id INT PRIMARY KEY NOT NULL AUTO_INCREMENT,
  fname VARCHAR(20) NOT NULL,
  lname VARCHAR(20),
  email VARCHAR(50) NOT NULL,
  pwd LONGTEXT,
  mode VARCHAR NOT NULL
) ENGINE=INNODB;

